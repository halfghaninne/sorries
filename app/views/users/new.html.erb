<%= render :partial => '/shared/mobile_nav_slideout' %>

<div class="container">
  
  <%= render :partial => '/shared/header' %>  

  <div>
  
    <%= form_for @user do |f| %>
      <%= f.label :username %> 
        <%= f.text_field(:username) %>
      <%= f.label :password %> 
        <%= f.password_field(:password) %>
      <%= f.submit "Sign Up" %>
    <% end %>

  </div>

</div>